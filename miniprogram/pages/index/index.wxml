<swiper bindchange="schange" circular="true">
  <swiper-item
    wx:for="{{ candidateList }}"
    wx:key="_id"
    bind:tap="tap"
    bind:longpress="long"
    data-fileid="{{ item.fileID }}"
    data-index="{{ index }}"
  >
    <image mode="aspectFit" src="{{ item.fileID }}"></image>
  </swiper-item>
</swiper>
